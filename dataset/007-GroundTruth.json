    {
      "name": "Library",
      "attributes": [
        "name: String",
        "address: Address"
      ],
      "operations": []
    },
    {
      "name": "Patron",
      "attributes": [
        "/name: FullName",
        "address: Address"
      ],
      "operations": []
    },
    {
      "name": "Catalog",
      "attributes": [],
      "operations": []
    },
    {
      "name": "Librarian",
      "attributes": [
        "/name: FullName",
        "address: Address",
        "position: String"
      ],
      "operations": []
    },
	{
	   "name": "Address"
	},
	{
	   "name": "Full Name"
	},
	{
	   "name": "Search"
