        "multiplicity_target": "1..*"
      },
      {
        "direction": "bidirectional",
        "source": "Book Item",
        "target": "Account",
        "multiplicity_source": "0.12",
        "multiplicity_target": "1"
      },
      {
        "direction": "bidirectional",
